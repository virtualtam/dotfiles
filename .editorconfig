root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.{json,xml,yaml,yml}]
indent_style = space
indent_size = 2

[settings.json]
indent_size = 4

[keybindings.json]
indent_size = 4

[*.ps1]
end_of_line = crlf
