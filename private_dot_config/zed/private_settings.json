// Zed settings
//
// For information on how to configure Zed, see the Zed
// documentation: https://zed.dev/docs/configuring-zed
//
// To see all of Zed's default settings without changing your
// custom settings, run `zed: open default settings` from the
// command palette (cmd-shift-p / ctrl-shift-p)
{
  "agent": {
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "openrouter",
      "model": "anthropic/claude-sonnet-4"
    },
    "version": "2"
  },
  "auto_install_extensions": {
    "asciidoc": true,
    "catppuccin-icons": true,
    "dockerfile": true,
    "elixir": true,
    "erlang": true,
    "fish": true,
    "git-firefly": true,
    "html": true,
    "ini": true,
    "just": true,
    "latex": true,
    "log": true,
    "lua": true,
    "make": true,
    "proto": true,
    "ruby": true,
    "ruff": true,
    "sql": true,
    "terraform": true,
    "toml": true
  },
  "buffer_font_size": 16,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "icon_theme": "Catppuccin Macchiato",
  "telemetry": {
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light",
    "dark": "One Dark"
  },
  "ui_font_size": 16
}
