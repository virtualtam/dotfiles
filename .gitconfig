[user]
	email = virtualtam@flibidi.org
	name = VirtualTam
[push]
	default = simple
[alias]
	br = branch
	ci = commit
	co = checkout
	st = status
	lg = log --color --graph --pretty=format:'%C(yellow)%h%Creset%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset%C(bold blue)<%an>%Creset'
	lh = log --color --graph --pretty=format:'%C(yellow)%H%Creset %Cgreen%cd%Creset %C(yellow)%d%Creset %s %C(bold blue)<%an>%Creset' --date=iso
	ll = log --pretty=format:'%C(yellow)%h %Cgreen%cr%Creset%C(yellow)%d%Creset %s %C(bold blue)<%an>%Creset' --stat
[core]
	excludesfile = ~/.gitignore_global
